{
  "name": "@bao-ui/cli",
  "version": "2.0.0",
  "description": "CLI for adding Bao UI components from the registry to your project",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "bao": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --shims",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "dev": "tsup src/index.ts --format esm --dts --watch"
  },
  "keywords": [
    "cli",
    "bao-ui",
    "base-ui",
    "components",
    "registry",
    "shadcn",
    "react"
  ],
  "author": "Bao To",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.4.2",
    "@types/node": "^20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
