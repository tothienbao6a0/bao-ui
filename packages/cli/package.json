{
  "name": "@bao/cli",
  "version": "0.1.0-alpha.0",
  "description": "CLI for scaffolding Bao UI components",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "bao": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "dev": "tsup src/index.ts --format esm --dts --watch"
  },
  "keywords": [
    "cli",
    "scaffolding",
    "code-generation",
    "react",
    "components"
  ],
  "author": "Bao To",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "ejs": "^3.1.9",
    "fs-extra": "^11.2.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "tsup": "^8.0.1",
    "typescript": "^5.4.2"
  }
}