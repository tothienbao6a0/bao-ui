{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "isolatedModules": true,
    "declaration": true,
    "declarationMap": true,
    "noEmit": true,
    "composite": true,
    "incremental": true,
    "baseUrl": "."
  },
  "exclude": ["node_modules"]
}
